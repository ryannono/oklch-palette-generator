{
  "name": "oklch-palette-generator",
  "version": "0.2.5",
  "type": "module",
  "packageManager": "pnpm@10.14.0",
  "license": "MIT",
  "description": "OKLCH-based color palette generator with Effect-ts",
  "keywords": [
    "oklch",
    "color",
    "palette",
    "generator",
    "design-system",
    "color-scheme",
    "perceptual",
    "accessibility",
    "effect-ts",
    "cli",
    "color-palette",
    "color-generator",
    "design-tokens",
    "ui-design",
    "color-theory"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ryannono/oklch-palette-generator.git"
  },
  "bugs": {
    "url": "https://github.com/ryannono/oklch-palette-generator/issues"
  },
  "homepage": "https://github.com/ryannono/oklch-palette-generator#readme",
  "publishConfig": {
    "access": "public"
  },
  "bin": {
    "oklch-palette-generator": "./build/esm/cli/index.js",
    "oklch-palette": "./build/esm/cli/index.js"
  },
  "files": [
    "build/",
    "patterns/",
    "README.md",
    "LICENSE"
  ],
  "exports": {
    "./package.json": "./package.json",
    "./patterns/*": "./patterns/*"
  },
  "scripts": {
    "dev": "tsx src/cli/index.ts generate",
    "codegen": "build-utils prepare-v2",
    "build": "pnpm build-esm && pnpm build-annotate && pnpm build-cjs && pnpm build-fix-shebang && build-utils pack-v2",
    "build-esm": "tsc -b tsconfig.build.json",
    "build-fix-shebang": "node -e \"const fs=require('fs');const path='build/esm/cli/index.js';let content=fs.readFileSync(path,'utf8');if(!content.startsWith('#!/usr/bin/env node')){fs.writeFileSync(path,'#!/usr/bin/env node\\n'+content);}\"",
    "build-cjs": "babel build/esm --plugins @babel/transform-export-namespace-from --plugins @babel/transform-modules-commonjs --out-dir build/cjs --source-maps",
    "build-annotate": "babel build/esm --plugins annotate-pure-calls --out-dir build/esm --source-maps",
    "check": "tsc -b tsconfig.json",
    "dep-graph": "tsx scripts/dependency-graph.ts",
    "lint": "eslint \"**/{src,test,examples,scripts,dtslint}/**/*.{ts,mjs}\"",
    "lint-fix": "pnpm lint --fix",
    "test": "vitest",
    "coverage": "vitest --coverage"
  },
  "dependencies": {
    "@clack/prompts": "^0.8.2",
    "@effect/cli": "^0.72.1",
    "@effect/platform": "^0.93.5",
    "@effect/platform-node": "^0.102.0",
    "clipboardy": "^5.0.1",
    "culori": "^4.0.1",
    "effect": "^3.19.8"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.8",
    "@babel/core": "^7.25.2",
    "@babel/plugin-transform-export-namespace-from": "^7.24.7",
    "@babel/plugin-transform-modules-commonjs": "^7.24.8",
    "@effect/build-utils": "^0.8.9",
    "@effect/eslint-plugin": "^0.3.2",
    "@effect/language-service": "latest",
    "@effect/vitest": "^0.27.0",
    "@eslint/compat": "1.1.1",
    "@eslint/eslintrc": "3.1.0",
    "@eslint/js": "9.10.0",
    "@types/culori": "^4.0.1",
    "@types/node": "^22.5.2",
    "@typescript-eslint/eslint-plugin": "^8.4.0",
    "@typescript-eslint/parser": "^8.4.0",
    "@vitest/coverage-v8": "^4.0.13",
    "babel-plugin-annotate-pure-calls": "^0.5.0",
    "eslint": "^9.10.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-codegen": "^0.28.0",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-sort-destructure-keys": "^2.0.0",
    "tsx": "^4.17.0",
    "typescript": "^5.6.2",
    "vitest": "^4.0.13"
  },
  "effect": {
    "generateExports": {
      "include": [
        "**/*.ts"
      ]
    },
    "generateIndex": {
      "include": [
        "**/*.ts"
      ]
    }
  },
  "pnpm": {
    "patchedDependencies": {}
  }
}